{
  "name": "cgppindel",
  "title": "cgppindel",
  "summary": "cgppindel",
  "dxapi": "1.0.0",
  "version": "1.0.0",
  "inputSpec": [
    {
      "name": "reference",
      "label": "reference genome",
      "class": "file",
      "optional": false,
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "simrep",
      "label": "simple repeats",
      "class": "file",
      "optional": false,
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "genes",
      "label": "gene bed file",
      "class": "file",
      "optional": false,
      "patterns": [
        "*"
      ],
      "help": ""
