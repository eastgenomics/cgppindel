      "class": "file",
      "optional": false,
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "outdir",
      "label": "Output directory",
      "class": "string",
      "optional": false,
      "help": ""
    }
  ],
  "outputSpec": [
    {
      "name": "output_vcf",
      "label": "Variant Call Format",
      "class": "file",
      "patterns": [
        "*"
