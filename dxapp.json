      "optional": false,
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "assembly",
      "label": "genome assembly version",
      "class": "string",
      "optional": false,
      "help": ""
    },
    {
      "name": "species",
      "label": "Species",
      "class": "string",
      "optional": false,
      "help": ""
    },
    {
      "name": "seqtype",
      "label": "Sequencing type",
      "class": "string",
      "optional": false,
      "help": ""
    },
    {
      "name": "filter",
